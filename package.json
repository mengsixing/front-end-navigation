{
  "name": "front-end-navigation",
  "version": "1.5.2",
  "description": "Chrome extensions for front-end development.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext js --ext vue ./src ./config --fix && stylelint \"./src/**/*.vue\" --fix",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "analysis": "cross-env NODE_ENV=analysis webpack --progress",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --progress",
    "build:chrome": "rimraf ./chrome-extends/* && cp ./google-extends-manifest.json ./chrome-extends/manifest.json && cross-env NODE_ENV=chrome webpack --progress",
    "test": "cross-env NODE_ENV=chrome BABEL_ENV=test karma start test/unit/karma.conf.js",
    "coverage": "cat test/unit/coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmjben/front-end-navigation.git"
  },
  "author": "lmjben",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmjben/front-end-navigation/issues"
  },
  "homepage": "https://github.com/lmjben/front-end-navigation#readme",
  "dependencies": {
    "element-ui": "^2.12.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "rimraf": "^3.0.0",
    "stylelint-webpack-plugin": "^1.0.2",
    "vue": "^2.6.10",
    "vue-loader": "15.7.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-merge": "^4.2.2",
    "cssnano": "^4.1.10",
    "autoprefixer": "^9.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "ejs": "^2.7.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "karma": "^4.4.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "url-loader": "^2.2.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
